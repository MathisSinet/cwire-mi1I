set terminal png size 1200,800 enhanced font "Helvetica,20"
set output 'graphs/graph.png'

set style data histogram
set style fill solid
set datafile separator '_'

set title "lv all minmax"
set boxwidth 0.9
set xtics rotate
set grid ytics

plot "tests/lv_all_minmax.csv" using 2:xtic(1) title "Capacit√©" linecolor rgb "#00FF00", \
    "tests/lv_all_minmax.csv" using 3 title "Consommation" linecolor rgb "#FF0000"

exit